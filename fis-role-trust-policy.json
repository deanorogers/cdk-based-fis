{
  "Version":"2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Service": [
          "fis.amazonaws.com"
        ]
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "107404535822"
        },
        "ArnLike": {
          "aws:SourceArn": "arn:aws:fis:us-east-1:107404535822:experiment/*"
        }
      }
    }
  ]
}
